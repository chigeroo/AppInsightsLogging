{
	"ApplicationInsights":
	{
		"InstrumentationKey": "<INSTRUMENTATION KEY>"
	},
	"StorageSettings":
	{
		"Connection": "<STORAGE CONNECTION>",
		"Container": "logging"
	},
	"Logging":
	{
		// For use with normal logger including AddApplicationInsights
		"LogLevel":
		{
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Warning"
		},
		//For use with AddApplicationInsightsTelemetryWorkerService
		"ApplicationInsights":
		{
			"LogLevel":
			{
				"Default": "Information"
			}
		}
	}
}